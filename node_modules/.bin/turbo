#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/zyk/personal/work/node_modules/.pnpm/turbo@1.9.3/node_modules/turbo/bin/node_modules:/home/zyk/personal/work/node_modules/.pnpm/turbo@1.9.3/node_modules/turbo/node_modules:/home/zyk/personal/work/node_modules/.pnpm/turbo@1.9.3/node_modules:/home/zyk/personal/work/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/zyk/personal/work/node_modules/.pnpm/turbo@1.9.3/node_modules/turbo/bin/node_modules:/home/zyk/personal/work/node_modules/.pnpm/turbo@1.9.3/node_modules/turbo/node_modules:/home/zyk/personal/work/node_modules/.pnpm/turbo@1.9.3/node_modules:/home/zyk/personal/work/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../turbo/bin/turbo"   "$@"
exit $?
